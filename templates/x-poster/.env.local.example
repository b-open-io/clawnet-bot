# X API - OAuth 2.0 (required for write operations: posting, liking, retweeting)
X_CLIENT_SECRET_ID=
X_CLIENT_SECRET=
X_REFRESH_TOKEN=

# X API - Bearer Token (required for read operations: search, mentions)
X_BEARER_TOKEN=

# AI Gateway (required for AI-generated content)
