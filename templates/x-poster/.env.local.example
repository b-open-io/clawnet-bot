# Bot identity (injected by clawnet bot deploy)
SIGMA_MEMBER_PRIVATE_KEY=

# Optional auth token for /api/tweet endpoint
X_POSTER_SHARED_TOKEN=

# ClawNet registry (optional)
CLAWNET_API_URL=
CLAWNET_AUTH_TOKEN=
